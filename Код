class Stack:
    def __init__(self):
        self.items = []

    def push(self, item):
        self.items.append(item)
        print("ok")

    def pop(self):
        if not self.is_empty():
            print(self.items.pop())
        else:
            print("error")

    def back(self):
        if not self.is_empty():
            print(self.items[-1])
        else:
            print("error")

    def size(self):
        print(len(self.items))

    def clear(self):
        self.items = []
        print("ok")

    def is_empty(self):
        return len(self.items) == 0

def print_menu():
    print("Меню команд:")
    print("push <n> - добавить в стек число n")
    print("pop - удалить из стека последний элемент")
    print("back - вывести значение последнего элемента в стеке")
    print("size - вывести количество элементов в стеке")
    print("clear - очистить стек")
    print("exit - завершить работу")
    print("Команда:")

stack = Stack()

print_menu()

while True:
    command = input().split()
    if command[0] == "push":
        if len(command) != 2:
            print("error")
        else:
            try:
                stack.push(int(command[1]))
            except ValueError:
                print("error")
    elif command[0] == "pop":
        stack.pop()
    elif command[0] == "back":
        stack.back()
    elif command[0] == "size":
        stack.size()
    elif command[0] == "clear":
        stack.clear()
    elif command[0] == "exit":
        print("bye")
        break
    else:
        print("error")
        print_menu()
